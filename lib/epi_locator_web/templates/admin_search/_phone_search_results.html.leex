<h5><%= number_of_search_results(@visible_search_results) %> </h5>

<ul class="search-results">
  <%= for phone_result <- @visible_search_results do %>
    <li>
      <span class="phone-number"><%= phone_result.phone_number %> </span>
      <span class="address"><%= phone_result.street %>, <%= phone_result.city %>, <%= phone_result.state %> <%= phone_result.zip_code %></span>
      </li>
  <% end %>
</ul>
